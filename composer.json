{
  "name": "kolinalabs/getnet-sdk",
  "description": "SDK da plataforma digital da GETNET",
  "minimum-stability": "stable",
  "license": "Apache-2.0",
  "type": "library",
  "require": {
    "php": ">=5.6.0"
  },
  "autoload": {
    "psr-4": {
      "Getnet\\": "src//Getnet"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Bruno Paz",
      "email": "brunopaz@azpay.com.br"
    },
    {
      "name": "Edson Nascimento",
      "email": "edson27bn@gmail.com"
    },
    {
      "name": "Gianluca Bine",
      "email": "gian_bine@hotmail.com"
    }
  ],
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
    "format": "vendor/bin/php-cs-fixer fix"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "friendsofphp/php-cs-fixer": "^2.14"
  }
}
